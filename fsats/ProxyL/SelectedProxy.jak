layer ProxyL;

import java.util.*;

import laydown.*;

import log.Log;

/** A pair of the best weapon found and 
 *  to which opfac to send the mission.
 */
public class SelectedProxy {
    private Proxy via;
    private Proxy weapon;

    public SelectedProxy( Proxy via, Proxy weapon )
        {
        this.via=via;
        this.weapon=weapon;
    }

    public Proxy getVia() {
        return via;
    }
    public Proxy getWeapon() {
        return weapon;
    }
}
