// tests mangleing of names within escapes in AST constructors

layer Escape;
 

import Jakarta.util.*;

SoUrCe  Escape "../escape.jak";

 abstract class top$$Escape {
   AST_Exp j$$Escape;
   AST_Exp i$$Escape = exp{ i + j + $exp( j$$Escape ) }exp;
}

SoUrCe  Escape "../escape.jak";

 class top extends  top$$Escape  {
   AST_Exp j$$Escape;
   AST_Exp i$$Escape = exp{ i + j + $exp( j$$Escape ) }exp;
}