#foreach($field in $listFields)
<TR BGCOLOR="white" CLASS="TableRowColor"> 
	<TD ALIGN="right" VALIGN="top" WIDTH="1%">
	<FONT SIZE="-1"> 
	<CODE> $field.modifiers() $field.type() </CODE> 
	</FONT>	
	</TD> 
	<TD>
	<CODE> <B> $field.name() </B> </CODE> 
 	<BR> $field.comment().textFirstSentenceTags()
 	</TD> 	
</TR> 
#end

