import Jakarta.util.*;
import java.util.*;

refines class not {
   public void reduce( cnfClause c ) {
      cterm t = new cterm(true);
      left.reduce(t);
      c.add(t);
   }

   public void reduce( ArrayList terms ) {
      cnfClause c = new cnfClause();
      reduce(c);
      terms.add(c);
   }
}
